    <dependencies>
      <dependency id="Wave" version="$DependencyVer$" />
    </dependencies>
  </metadata>
  <files>
    <file src="$BinDirInclude$\$Configuration$\ImplicitNullability.*" target="dotFiles" />
  </files>
</package>
