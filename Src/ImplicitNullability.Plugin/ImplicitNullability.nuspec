  <metadata>
    <id>ReSharper.ImplicitNullability</id>
    <version>$Version$</version>
    <title>Implicit Nullability</title>
    <authors>ulrichb</authors>
    <licenseUrl>https://github.com/ulrichb/ImplicitNullability/blob/master/LICENSE</licenseUrl>
