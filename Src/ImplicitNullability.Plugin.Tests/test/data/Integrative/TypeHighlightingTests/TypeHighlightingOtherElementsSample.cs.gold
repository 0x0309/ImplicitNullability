using System;
using JetBrains.Annotations;

// ReSharper disable NotNullMemberIsNotInitialized

namespace ImplicitNullability.Plugin.Tests.test.data.Integrative.TypeHighlightingTests
{
    public class TypeHighlightingOtherElementsSample
    {
        public class Constructor
        {
            public Constructor(|string|(0) a, [CanBeNull] string b)
            {
                Console.WriteLine(a + b);
            }
        }

        public class Delegates
        {
            public delegate |string|(1) SomeDelegate(|string|(2) a);

            [CanBeNull]
            public delegate string SomeNullableDelegate([CanBeNull] string a);
        }

        // Atm. IN doesn't support fields & properties

        public class Fields
        {
            [NotNull]
            public |string|(3) SomeField;

            [CanBeNull]
            public string SomeCanBeNullField;
        }

        public abstract class PropertiesBase
        {
            [NotNull]
            public abstract |string|(4) VirtualPropertyWithExplicitNullabilityInBase { get; set; }
        }

        public class Properties : PropertiesBase
        {
            [NotNull]
            public |string|(5) SomeProperty { get; set; }

            [CanBeNull]
            public string SomeCanBeNullProperty { get; set; }

            [NotNull]
            public |string|(6) SomeExpresssionBodyProperty => "";

            [NotNull]
            public |string|(7) SomeGetterOnlyProperty { get; } = "";

            public override |string|(8) VirtualPropertyWithExplicitNullabilityInBase { get; set; }
        }

        public class Indexers
        {
            [NotNull]
            public |string|(9) this[|string|(10) a] => "";

            [CanBeNull]
            public string this[[CanBeNull] object b] => "";
        }
    }
}

